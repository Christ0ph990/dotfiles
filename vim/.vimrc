" 基本設定
source ~/.vim/lib/basic.vimrc

" エンコーディング設定
source ~/.vim/lib/encording.vimrc

" 履歴・バックアップ設定
source ~/.vim/lib/history.vimrc

" 表示設定
source ~/.vim/lib/display.vimrc

" カラー設定
source ~/.vim/lib/colors.vimrc

" 折り畳み設定
source ~/.vim/lib/folding.vimrc

" インデント設定
source ~/.vim/lib/indent.vimrc

" 補完設定
source ~/.vim/lib/completion.vimrc

" キーマップ設定
source ~/.vim/lib/keymap.vimrc

" プラグイン設定
" source ~/.vim/lib/plugins.vimrc
