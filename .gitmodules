[submodule "zsh/.zsh/modules/zsh-completions"]
	path = zsh/.zsh/modules/zsh-completions
	url = git://github.com/zsh-users/zsh-completions.git
	ignore = dirty
[submodule "zsh/.zsh/modules/auto-fu"]
	path = zsh/.zsh/modules/auto-fu
	url = git://github.com/hchbaw/auto-fu.zsh.git
	ignore = dirty
[submodule "vimperator/.vimperator/vimpr"]
	path = vimperator/.vimperator/vimpr
	url = git://github.com/vimpr/vimperator-plugins.git
	ignore = dirty
[submodule "zsh/.zsh/modules/zaw"]
	path = zsh/.zsh/modules/zaw
	url = git://github.com/zsh-users/zaw.git
	ignore = dirty
[submodule "zsh/.zsh/modules/cdd"]
	path = zsh/.zsh/modules/cdd
	url = git://github.com/m4i/cdd.git
	ignore = dirty
[submodule "vim"]
	path = vim
	url = git@github.com:fujimogn/vimrc.git
	ignore = dirty
